{
  "name": "serverkess-clean-api",
  "version": "1.0.0",
  "description": "Serverless Clean api",
  "main": "index.js",
  "scripts": {
    "dev": "npx sls offline --dontPrintOutput --stage dev",
    "deploy:dev": "yarn build && npx sls deploy --stage development",
    "deploy::prod": "yarn build && npx sls deploy --stage production",
    "migrate": "npx prisma migrate save --experimental && npx prisma migrate up --experimental",
    "generate": "npx prisma introspect && npx prisma generate",
    "lint": "eslint --ext .js,.ts,.gql,.graphql src/",
    "prettier": "prettier --ignore-path .gitignore  \"**/*.+(js|ts|json)\"",
    "format": "npm run prettier -- --list-different",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Lucas Santos",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/node": "^7.8.7",
    "@prisma/cli": "^2.0.0-beta.5",
    "@types/aws-lambda": "^8.10.59",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^14.0.25",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "serverless": "^1.71.1",
    "serverless-dotenv-plugin": "^2.4.2",
    "serverless-offline": "^6.7.0",
    "serverless-prune-plugin": "^1.4.2",
    "serverless-webpack": "^5.3.4",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^4.0.2",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-beta.5",
    "class-validators": "^1.0.1",
    "date-fns": "^2.15.0",
    "firebase-admin": "^8.10.0",
    "graphql": "^15.0.0",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13"
  }
}
